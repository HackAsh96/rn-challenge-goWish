{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": ["esnext", "ES2022"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-native"
  },
  "include": [
    "./src/**/*",
    "./*.tsx",
    "./*.ts"
  ],
  "exclude": [
    "node_modules",
    "node_modules/*",
  ]
}